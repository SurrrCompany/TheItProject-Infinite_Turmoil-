if keyboard_check(W){vspeed-=1}
if keyboard_check(S){vspeed+=1}
if keyboard_check(A){hspeed-=1}
if keyboard_check(D){hspeed+=1}

//Extra
if speed &gt;4 {speed=4}
if speed &lt;-4 {speed=-4}
if vspeed =0 &amp;&amp; hspeed =0 {image_index = 0}

//Sprite = Direction
if keyboard_check(W)
sprite_index = spr_walk_up;
if keyboard_check(S)
sprite_index = spr_walk_dw;
if keyboard_check(A)
sprite_index = spr_walk_left;
if keyboard_check(D)
sprite_index = spr_walk_right;
